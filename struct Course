#include <iostream>
using namespace std;


struct Course
{
	int CourseNumber;
	char grade;

};
	
int main()
	{
	Course mycourse;
	mycourse.CourseNumber = 125;
	mycourse.grade = 'A';

	//display values stored in data members
	cout << "I took MAC " << mycourse.CourseNumber;
	cout << " and I got a(n) " << mycourse.grade << endl;


	cout << endl;

	system("pause");
	return 0;
	}
